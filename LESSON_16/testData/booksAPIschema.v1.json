{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer"
        },
        "title": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "pageCount": {
            "type": "integer"
        },
        "excerpt": {
            "type": "string"
        },
        "publishDate": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "id",
        "title",
        "description",
        "pageCount",
        "excerpt",
        "publishDate"
    ]
}


